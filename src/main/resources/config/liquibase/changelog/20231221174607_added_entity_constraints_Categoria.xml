<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Categoria.
    -->
    <changeSet id="20231221174607-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="categoria_id"
                                 baseTableName="rel_categoria__productos"
                                 constraintName="fk_rel_categoria__productos__categoria_id"
                                 referencedColumnNames="id"
                                 referencedTableName="categoria"
                                 />

        <addForeignKeyConstraint baseColumnNames="productos_id"
                                 baseTableName="rel_categoria__productos"
                                 constraintName="fk_rel_categoria__productos__productos_id"
                                 referencedColumnNames="id"
                                 referencedTableName="producto"
                                 />
    </changeSet>
</databaseChangeLog>
